  <decision id="Decision_0palmjd" name="HEARING_JUDGE_ROLE_CATEGORY">
    <variable id="InformationItem_1h7skk0" name="HEARING_JUDGE_ROLE_CATEGORY" typeRef="string" />
    <literalExpression id="LiteralExpression_1ezthzx">
      <text>if (
    sort(
        list: caseData.hearingDetails[
            date and time(item.value.startDate) 
            &lt;= date and time(substring(string(date(now())), 1, 10) + "T" + substring(string(time(now())), 1, 8))
        ].value,
        precedes: function(x, y) x.startDate &gt;= y.startDate
    )[1].judgeAndLegalAdvisor.judgeTitle = "LEGAL_ADVISOR" 
    or matches(
        sort(
            list: caseData.hearingDetails[
                date and time(item.value.startDate) 
                &lt;= date and time(substring(string(date(now())), 1, 10) + "T" + substring(string(time(now())), 1, 8))
            ].value,
            precedes: function(x, y) x.startDate &gt;= y.startDate
        )[1].judgeAndLegalAdvisor.judgeEmailAddress, "@justice.gov.uk", "i")
) 
then "LEGAL_OPERATIONS" 
else if (
    (caseData.hearingDetails = null or count(caseData.hearingDetails) = 0 or sort(
        list: caseData.hearingDetails[
            date and time(item.value.startDate) 
            &lt;= date and time(substring(string(date(now())), 1, 10) + "T" + substring(string(time(now())), 1, 8))
        ].value,
        precedes: function(x, y) x.startDate &gt;= y.startDate
    )[1] = null) and    
    (matches(caseData.allocatedJudge.judgeEmailAddress, "@justice.gov.uk", "i")
    or caseData.allocatedJudge.judgeTitle = "LEGAL_ADVISOR")
) 
then "LEGAL_OPERATIONS" 
else "JUDICIAL"</text>
    </literalExpression>
  </decision>
  <decision id="Decision_0a1wkek" name="ALLOCATED_JUDGE_ROLE_CATEGORY">
    <variable id="InformationItem_10mxw4q" name="ALLOCATED_JUDGE_ROLE_CATEGORY" typeRef="string" />
    <literalExpression id="LiteralExpression_1ff8oiq">
      <text>if (
  caseData.allocatedJudge != null
    and (matches(caseData.allocatedJudge.judgeEmailAddress, "@justice.gov.uk", "i") or caseData.allocatedJudge.judgeTitle = "LEGAL_ADVISOR")
)
then "LEGAL_OPERATIONS" 
else "JUDICIAL"</text>
    </literalExpression>
  </decision>
